<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFDE59"
    tools:context=".inicio.RegisterFragment">

    <LinearLayout
        android:id="@+id/frg_explora__sup_layout"
        android:layout_width="442dp"
        android:layout_height="57dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="215dp"
        android:background="@drawable/supbar"
        android:orientation="horizontal"
        android:weightSum="100"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.117">

        <ImageButton
            android:id="@+id/frg_register_btn_back"
            android:layout_width="60dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:src="@drawable/ic_back" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="321dp"
        android:layout_marginBottom="71dp"
        android:text="@string/btn_registro"
        android:textAppearance="@style/title"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frg_explora__sup_layout"
        app:layout_constraintVertical_bias="0.476" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="486dp"
        android:layout_height="571dp"
        android:layout_marginTop="293dp"
        android:background="@drawable/btn_round_inicio_blanco"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.474">

        <EditText
            android:id="@+id/editTextTextEmailAddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="57dp"
            android:layout_marginTop="80dp"
            android:drawableEnd="@drawable/ic_usuario"
            android:ems="10"
            android:hint="@string/email"
            android:inputType="textEmailAddress" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="57dp"
            android:layout_marginTop="40dp"
            android:drawableEnd="@drawable/ic_candado"
            android:ems="10"
            android:hint="@string/pass"
            android:inputType="textPassword" />

        <EditText
            android:id="@+id/editTextTextPassword3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="57dp"
            android:layout_marginTop="40dp"
            android:drawableEnd="@drawable/ic_candado"
            android:ems="10"
            android:hint="@string/pass"
            android:inputType="textPassword" />

        <Button
            android:id="@+id/frg_registro_btn_sigin"
            android:layout_width="189dp"
            android:layout_height="81dp"
            android:layout_marginHorizontal="147dp"
            android:layout_marginTop="60dp"
            android:layout_marginBottom="120dp"
            android:layout_weight="75"
            android:background="@drawable/btn_round_inico_negro_"
            android:text="@string/inicio_sesion"
            android:textColor="@color/white"
            android:textSize="20sp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>