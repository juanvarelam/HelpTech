<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="8dp"
    android:background="#FFFFFF"
    tools:context=".ConfigFragment">

    <LinearLayout
        android:id="@+id/frg_config_title_lin"
        android:layout_width="0dp"
        android:layout_height="64dp"
        android:background="@drawable/supbar"
        android:orientation="horizontal"
        android:weightSum="100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/frg_notificacion_btn_back"
            android:layout_width="64dp"
            android:layout_height="match_parent"
            android:layout_weight="10"
            android:background="@null"
            app:srcCompat="@drawable/ic_back" />

        <TextView
            android:id="@+id/frg_notificacion_title"
            style="@style/title"
            android:layout_width="238dp"
            android:layout_height="64dp"
            android:layout_weight="60"
            android:gravity="center_horizontal|center_vertical"
            android:text=""
            android:textStyle="bold"
            tools:text="@string/Notificaciones" />

        <ImageButton
            android:id="@+id/frg_notificacion_btn_user"
            android:layout_width="64dp"
            android:layout_height="match_parent"
            android:layout_weight=""
            android:background="@null"
            android:src="@drawable/ic_usuario_de_perfil"
            android:contentDescription="TODO" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/notificacion_main_lin_notif"
        android:layout_width="match_parent"
        android:layout_height="124dp"
        android:layout_marginTop="24dp"
        android:padding="16dp"
        app:layout_constraintTop_toBottomOf="@+id/frg_config_title_lin"
        tools:layout_editor_absoluteX="0dp">

        <ImageButton
            android:id="@+id/frg_notificacion_btn_Visituser"
            android:layout_width="86dp"
            android:layout_height="match_parent"
            android:layout_weight="30"
            android:background="@null"
            app:srcCompat="@drawable/ic_usuario_de_perfil"
            android:contentDescription="@string/todo" />

        <TextView
            android:id="@+id/frg_notificacion_notif"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="70"
            android:text="@string/txtNotificacion"
            android:textAlignment="center" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_margin="8dp"
        android:background="#191818"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="@id/notificacion_main_lin_notif"
        app:layout_constraintStart_toStartOf="@id/notificacion_main_lin_notif"
        app:layout_constraintTop_toBottomOf="@+id/notificacion_main_lin_notif" />

    <LinearLayout
        android:id="@+id/frg_main_menu_lin"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@drawable/supbar"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/frg_main_menu_home_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:layout_weight="10"
            android:background="@null"
            android:src="@drawable/ic_casa_silueta_negra_sin_puerta"
            android:contentDescription="TODO" />

        <ImageButton
            android:id="@+id/frg_main_menu_buscar_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="56dp"
            android:layout_weight="10"
            android:background="@null"
            android:src="@drawable/ic_buscar" />


        <ImageButton
            android:id="@+id/frg_main_menu_notif_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="56dp"
            android:layout_marginTop="8dp"
            android:layout_weight="10"
            android:background="@null"
            android:src="@drawable/ic_notificacion2"
            android:contentDescription="TODO" />

        <ImageButton
            android:id="@+id/frg_main_menu_usuario_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:layout_marginHorizontal="42dp"
            android:layout_weight="10"

            android:background="@null"
            android:src="@drawable/ic_usuario_de_perfil" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/frg_main_menu_lin_mas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/frg_main_menu_lin"
        tools:layout_editor_absoluteX="1dp">

        <ImageButton
            android:id="@+id/frg_main_mas_btn"
            android:layout_width="match_parent"
            android:layout_height="114dp"
            android:layout_gravity="clip_horizontal"
            android:background="@null"
            android:scaleType="fitCenter"
            android:src="@drawable/ic_mas1"
            android:stateListAnimator="@null"
            tools:visibility="visible"
            android:contentDescription="TODO" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>